{#
/**
 * @file planet_entity.html.twig
 * Default theme implementation to present Planet entity data.
 *
 * This template is used when viewing Planet entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_planet_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('planet_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
